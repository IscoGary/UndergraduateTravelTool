        <activity android:name="com.wego.userActivity.CollectionTravelNotes.CollectionTravelNotesActivity">
        </activity>
        <activity android:name="com.wego.userActivity.MyNotes.MyNotesActivity">
        </activity>
        <activity android:name="com.wego.mainFragment.YJActivity"></activity>
    </application>

</manifest>